#include <iostream>
 
using namespace std;


#include<iostream>

int main()
{
    int nums[5];
    for(int i = 0; i < 5; i++)
    {
        cout << "Digite o numero: \n";
        cin >> nums[i];
    }
    cout << "Numeros escolhidos: \n";
    for(int i : nums)
    {
        cout << i << endl;
    }
}

int main()
{
    float notas[8];
    float nota;
    for(int i = 0; i <= 8; i++)
    {
        std::cout << "Digita a nota ai pae: \n";
        std::cin >> nota;
        notas[i] = nota;
    }

    float soma_nota;
    for(int i = 0; i <= 8; i++)
    {
        soma_nota += notas[i];
    }

    float media = soma_nota / 8;
    if(media >= 7)
    {
        std::cout << "Pasou fedendo, média: " << media << "\n";
    }
    else
    {
        std::cout << "Silascou kkkkk, media: " << media << " nota baixa da peste ";
    }

    return 0;
}

int main() {
    int num[8];
    for(int i = 0; i < 6; i ++){
        cout << "Digite um número: ";
        cin >> num[i];
    }

    int maior = num[0];
    int menor = num[0];

    for(int i = 1; i < 6; i++){
        if(num[i]> maior){
            maior = num[i];
        }
        if(num[i] < menor){
            menor = num[i];
        }
}

    cout << "Maior valor: " << maior << endl;
    cout << "Menor valor: " << menor;
}
